{
  "expo": {
    "name": "Simple",
    "slug": "simplefitnessappbeta",
    "version": "1.7.9",
    "orientation": "portrait",
    "icon": "./assets/simpleicon.png",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/simpleicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "tr.com.basarsubasi.simplefitnessapp"
    },
    "android": {
      "versionCode": 51,
      "blockedPermissions": [
        "android.permission.INTERNET",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.RECORD_AUDIO"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/simpleicon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "tr.com.basarsubasi.simplefitnessapp"
    },
    "web": {
      "favicon": "./assets/simpleicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "2298891a-04aa-41a6-910a-1c540f256658"
      }
    },
    "plugins": [
      "expo-sqlite",
      "expo-build-properties",
      [
        "expo-notifications",
        {
          "icon": "./assets/notificationicon.png",
          "color": "#000000",
          "enableBackgroundRemoteNotifications": false
        }
      ]
    ],
    "owner": "jiboli"
  }
}
